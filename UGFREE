local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/shlexware/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({
	Name = "UG HUB FREE VERSION",
	LoadingTitle = "UG HUB FREE VERSION",
	LoadingSubtitle = "by Ugljesa",
	ConfigurationSaving = {
	   Enabled = true,
	   FolderName = nil, 
	   FileName = "UG HUB FREE VERSION"
	},
	Discord = {
	   Enabled = false,
	   Invite = "ABCD", 
	   RememberJoins = true 
	},
	KeySystem = true, 
	KeySettings = {
	   Title = "|GPO BEST FREE HUB|",
	   Subtitle = "Key System",
	   Note = "Creator Ugljesa",
	   FileName = "Key",
	   SaveKey = true,
	   GrabKeyFromSite = false, 
	   Key = "UGFREE"
	}
 })
 local Tab = Window:CreateTab("Main", 4483362458)

 local ESP = Tab:CreateButton({
	Name = "ESP",
	Callback = function()
		local Players = game:GetService("Players")
			local RunService = game:GetService("RunService")
			local Highlight = Instance.new("Highlight")
			Highlight.Name = "Highlight"
			function ApplyToCurrentPlayers()
    			for i,player in pairs(Players:GetChildren()) do 
        			repeat wait() until player.Character
        				if not player.Character:FindFirstChild("HumanoidRootPart"):FindFirstChild("Highlight") then
            				local HighlightClone = Highlight:Clone()
            				HighlightClone.Adornee = player.Character
            				HighlightClone.Parent = player.Character:FindFirstChild("HumanoidRootPart")
            				HighlightClone.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop
            				HighlightClone.Name = "Highlight"
        end
    end
end    
RunService.Heartbeat:Connect(function()
    ApplyToCurrentPlayers()
end)

	end,
 })
 local Button = Tab:CreateButton({
	Name = "Infinity Jump Dont Work",
	Callback = function()
	-- The function that takes place when the button is pressed
	end,
 })
 
 Rayfield:Notify({
	Title = "Welcome to UG HUB",
	Content = "UG HUB FREE VERSION ",
	Duration = 6.5,
	Image = 4483362458,
	Actions = { -- Notification Buttons
	   Ignore = {
		  Name = "Okay!",
		  Callback = function()
		  print("Notification Works")
	   end
	},
 },
 })
 local Button = Tab:CreateButton({
	Name = "WalkSpeed Hub",
	Callback = function()
		local s_rs = game:GetService("RunService")
		local l_plr = game:GetService("Players").LocalPlayer
		
		
		
		local l_humrp = l_plr.Character and l_plr.Character:FindFirstChild("HumanoidRootPart")
		local l_hum = l_plr.Character and l_plr.Character:FindFirstChild("Humanoid")
		
		local resp_con = l_plr.CharacterAdded:Connect(function(c) 
			l_humrp = c:WaitForChild("HumanoidRootPart",3)
			l_hum = c:WaitForChild("Humanoid",3)
		end)
		
		local function dnec(signal) 
			local s = {}
			for _, con in ipairs(getconnections(signal)) do 
				local func = con.Function
				if (func and islclosure(func)) then
					if (not is_synapse_function(func)) then 
						s[#s+1] = con
						con:Disable() 
					end
				end
			end
			return s
		end
		
		
		local speed_amnt = 5
		
		local ui = loadstring(game:HttpGet('https://raw.githubusercontent.com/topitbopit/rblx/main/ui-stuff/jeff_2.lua'))()
		ui:SetColors('streamline')
		
		
		
		local w = ui:NewWindow('WalkSpeed Hub',250,200)
		
		local m = w:NewMenu('WalkSpeed')
		local t = m:NewToggle('Toggle WalkSpeed')
		local s = m:NewSlider('WalkSpeed Amount',1,10)
		
		t:SetTooltip("Toggle Walk")
		s:SetTooltip("WalkSpeed")
		
		m:NewLabel()
		m:NewLabel('Ugljesa WalkSpeed Free')
		
		s.OnValueChanged:Connect(function(v) 
			speed_amnt = v
		end)
		
		do
			local a,b
			t.OnToggle:Connect(function(t)
				if (t) then
					a = dnec(l_humrp.Changed)
					b = dnec(l_humrp:GetPropertyChangedSignal("CFrame"))
					
					s_rs:BindToRenderStep("speed",2000,function(dt)
						l_humrp.CFrame += l_hum.MoveDirection*dt*5*speed_amnt
					end)
				else
					s_rs:UnbindFromRenderStep("speed")
					
					for i,v in ipairs(a) do 
						v:Enable()
					end
					for i,v in ipairs(b) do 
						v:Enable()
					end
				end
			end)
		end
		
		ui.Exiting:Connect(function() 
			for i,v in ipairs(ui:GetAllToggles()) do 
				if (v:IsEnabled()) then
					v:Disable()
				end
			end
			resp_con:Disconnect()
		end)
		
		ui:Ready()
	end,
 })


getgenv().SecureMode = truee
